# QuantPulse Environment Variables
# Copy this file to .env and fill in your values

# Cloudflare R2 Configuration (for Becker analytics)
R2_ACCOUNT_ID=
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_BUCKET_NAME=quantpulse-data

# Database (Neon PostgreSQL)
# Get this from https://neon.tech
DATABASE_URL=postgresql://user:password@host/quantpulse?sslmode=require

# Server Configuration
PORT=3002
NODE_ENV=development
